        series:

          #**
          #* Interface for summing any type of value, as long as it is a large integer.
          #*
          #* @author jdrago
          #*
          ReallyLargeIntegerSummerInterface: class ReallyLargeIntegerSummerInterface
            constructor: (@delegate, abstract, synchronized, transient, inject, volatile) ->
              this.delegate["singleton"] = com.jdrago.euler.types.soundsLikeFoods.GUAVA

          #**
          #* Complete implementation of com.jdrago.euler.math.series.ReallyLargeIntegerSummerInterface
          #*
          #* @author jdrago
          #*
          ReallyLargeIntegerSummerInstantiation: class ReallyLargeIntegerSummerInstantiation extends ReallyLargeIntegerSummerInterface
            #**
            #* Basic constructor for com.jdrago.euler.math.series.ReallyLargeIntegerSummerInterface
            #*
            #* @author jdrago
            #*
            constructor: ->
              #**
              #* Initialize the sum to zero.
              #*
              #* @author jdrago
              #*
              @sum = 0

            #**
            #* Adds a new really large integer value to the current sum.
            #*
            #* @author jdrago
            #*
            add: (n) ->
              #**
              #* Now add n to sum.
              #*
              #* @author jdrago
              #*
              @sum += n

            #**
            #* Returns the sum.
            #*
            #* @author jdrago
            #*
            getSum: ->
              return @sum


          #**
          #* Generates an object which implements the interface com.jdrago.euler.math.series.ReallyLargeIntegerSummerInterface
          #*
          #* @author jdrago
          #*
          ReallyLargeIntegerSummerInstantiationFactory: class ReallyLargeIntegerSummerInstantiationFactory
            #**
            #* Basic constructor for com.jdrago.euler.math.series.ReallyLargeIntegerSummerInstantiationFactory
            #*
            #* @author jdrago
            #*
            constructor: ->

            #**
            #* Constructs an object which implements the interface com.jdrago.euler.math.series.ReallyLargeIntegerSummerInterface
            #*
            #* @author jdrago
            #*
            buildSummer: ->
              #**
              #* Now construct an object that implements the interface com.jdrago.euler.math.series.ReallyLargeIntegerSummerInterface and return it.
              #*
              #* @author jdrago
              #*
              return new com.jdrago.euler.math.series.ReallyLargeIntegerSummerInstantiation


problem.answer = ->

  summerFactory = new com.jdrago.euler.math.series.ReallyLargeIntegerSummerInstantiationFactory
  reallyLargeIntegerSummerInterfaceImplementation = summerFactory.buildSummer()

  for n in numbers
    reallyLargeIntegerSummerInterfaceImplementation.add(n)
  sum = reallyLargeIntegerSummerInterfaceImplementation.getSum()

  # JDRAGO 2014/06/20 disabled/replaced sloppy code which was bereft of layers of bureaucracy
